%% arrow library

/arrowdict 17 dict def      % arguments:   tailx taily tipx tipy mapsto
arrowdict /amtrx matrix put %       make mapsto = 1 for a `mapsto tail`
                            %                   = 0 for nothing on tail
                            %                   = 2 for two headed arrow
/setarrow {arrowdict begin /tailhalfwidth exch def
                           /headhalfwidth exch def
                           /headlength exch def end} bind def
/setarrowmode {arrowdict begin /mapsto exch def end} bind def

/arrowhead
 {arrowdict begin
   /angle exch def
   /savematrix amtrx currentmatrix def
   strans
   angle rotate
   newpath headlength -.4 mul headhalfwidth moveto
   headlength -.4 mul headhalfwidth neg lineto
   headlength .6 mul 0 lineto
   closepath fill
   savematrix setmatrix
   end
}def

/arrow
 {arrowdict begin
   /tipy exch def                     /tipx exch def
   /taily exch def                    /tailx exch def
   /dx tipx tailx sub xscale mul def  /dy tipy taily sub yscale mul def
   /arrowlength dx dx mul dy dy mul add sqrt def
   /angle dy dx atan def
   /base arrowlength headlength sub def
   /savematrix amtrx currentmatrix def
   tailx taily strans
   angle rotate
   mapsto 1  eq
      {np 0  tailhalfwidth neg  mt
          0  tailhalfwidth  ls} if
   mapsto 2  eq
      {np headlength  headhalfwidth neg  mt
          headlength  headhalfwidth  li
           0               0         li
           closepath                 fill} if

   np base 0                mt
   base headhalfwidth neg   li
   arrowlength 0            li
   base headhalfwidth       li
   closepath  fill
   np base 0                mt
   0 0                      ls
   savematrix setmatrix
   end
}def

%% arrow initialization 

     6 1.5 3 setarrow
     0 setarrowmode


